
@{
    ViewBag.Title = "Not Found";
    Layout = "~/Views/Shared/_JBGLayout.cshtml";
}

@if (User.IsInRole("Admin"))
{
    ViewBag.Title = "Error";
    Layout = "~/Areas/Admin/Views/Shared/_RealEstate.cshtml";

    <section class="content">

        <div class="error-page">
            <h2 class="headline text-red">400</h2>

            <div class="error-content">
                <h3><i class="fa fa-warning text-red"></i>We couldn't find the page you're looking for</h3>

                <p>
                    This could be due to a spelling error in the URL or a removed page,
                    you may <a href="@Url.Action("index", "Home", new { Area = "" })">return to dashboard</a>
                </p>


            </div>
        </div>
        <!-- /.error-page -->

    </section>
}
else if (User.IsInRole("Agent"))
{
    ViewBag.Title = "Error";
    Layout = "~/Areas/Coordinator/Views/Shared/_AgentLayout.cshtml";
    <section class="content">

        <div class="error-page">
            <h2 class="headline text-red">400</h2>

            <div class="error-content">
                <h3><i class="fa fa-warning text-red"></i>We couldn't find the page you're looking for</h3>

                <p>
                    This could be due to a spelling error in the URL or a removed page, you may <a href="@Url.Action("index", "Home", new { Area = "" })">return to dashboard</a>
                </p>


            </div>
        </div>
        <!-- /.error-page -->

    </section>
}
else
{

<link href="https://fonts.googleapis.com/css?family=Heebo:300,400,500,700,800,900" rel="stylesheet">
<link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,700,900" rel="stylesheet">

<style>
    .bbtn {
        font-size: 13px !important;
        line-height: 20px !important;
        padding: 6px 19px !important;
    }
    .circle-bullet {
        display: none !important;
    }
    .tparrows {
        display: none !important;
    }
</style>

<link href="~/201Murray/TempFIles/dropnav.css" rel="stylesheet" />

<!-- - - - - - - - - - - - - - Revolution Slider - - - - - - - - - - - - - - - - -->

<div class="rev-slider-wrapper">

    <div id="rev-slider" class="rev-slider" data-version="5.0">

        <ul>
            <li data-transition="slidevertical" data-slotamount="1" data-hideafterloop="0" data-hideslideonmobile="off" data-easein="default" data-easeout="default" data-masterspeed="1500" data-rotate="0">


                <img src="~/images/404.jpg" class="rev-slidebg" alt="" />
                <!-- - - - - - - - - - - - - - Layer 1 - - - - - - - - - - - - - - - - -->



                <div class="tp-caption tp-resizeme"
                     data-x="['center','center','center','center']" data-hoffset="['0','0','0','0']"
                     data-y="['middle','middle','middle','middle']" data-voffset="160"
                     data-whitespace="nowrap"
                     data-frames='[{"delay":950,"speed":2000,"frame":"0","from":"y:50px;opacity:0;","to":"o:1;","ease":"Power3.easeOut"},{"delay":"wait","speed":1000,"frame":"999","to":"y:[175%];","mask":"x:inherit;y:inherit;s:inherit;e:inherit;","ease":"Power2.easeInOut"}]'
                     data-responsive_offset="on"
                     data-elementdelay="0.05">
                    <a href="@Url.Action("Brand", "Home", new { Area = "" })" class="btn bbtn style-3">Go Back Home</a>
                </div>

                <!-- - - - - - - - - - - - - - End of Layer 3 - - - - - - - - - - - - - - - - -->

            </li>
        </ul>

    </div>

</div>

@section Scripts{

    <script src="~/201Murray/plugins/revolution/js/jquery.themepunch.tools.min.js?ver=5.0"></script>
    <script src="~/201Murray/plugins/revolution/js/jquery.themepunch.revolution.min.js?ver=5.0"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>


}

}