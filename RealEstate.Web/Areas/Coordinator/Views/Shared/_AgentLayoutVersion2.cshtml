    <div style="display:none">
        @{Html.RenderAction("UserDetails", "ControlPanel", new { area = "Admin" });}
    </div>
    <div class="content">
        <!-- Optional JavaScript -->
        <!-- jQuery first, then Popper.js, then Bootstrap JS -->
 
        <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>
        @Scripts.RenderFormat(
    "<script src='https://justbere-d4d9.kxcdn.com{0}' ></script>",
    "~/bundles/jquery")

        <script src="~/Content/NewVersion/Admin/js/bootstrap/bootstrap.min.js"></script>
        <script src="~/Scripts/respond.js"></script>
    
        <script src="~/Content/NewVersion/Admin/js/custom.js"></script>
        <script src="/Content/adminjs/app.min.js"></script>
        <script src="/Scripts/CustomAjax.js"></script>
        <script src="https://code.jquery.com/jquery-migrate-3.0.0.min.js"></script>
        <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
        @RenderBody();


     
    </div>
   


   

  

    <script type="text/javascript">
        $(document).ready(function () {
            $('.img-user-new').attr('src', $('#user-img').attr('src'));
        })

