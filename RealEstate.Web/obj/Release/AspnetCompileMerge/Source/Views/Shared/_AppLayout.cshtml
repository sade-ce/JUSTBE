    <link rel="icon" type="image/png" href="~/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="~/favicon-16x16.png" sizes="16x16" />
  <style>


    </style>

</head>
<body>

    <div class="page-wrapper">
        <div class="dashboard-login-wrapper">
            <div class="dashboard-login-inner">
                <div class="dashboard-login-left">
                    @RenderBody()
                    <!-- /.dashboard-footer -->
                </div><!-- /.dashboard-login-left -->
                <div class="dashboard-login-right">
                    <div class="dashboard-login-right-content">
                        @*<a href="index.html"><i class="fa fa-long-arrow-left"></i> Back To Front End</a>*@

                        @*<h1>Villareal Back End Interface</h1>
                        <h2>
                            Mauris ut rutrum lacus. Donec vulputate, lorem et volutpat consectetur, sapien mauris consectetur sapien, eu sagittis dui quam id justo.
                        </h2>*@
                    </div><!-- /.dashboard-login-right-content -->
                </div><!-- /.dashboard-login-right -->
            </div><!-- /.dashboard-login -->
        </div><!-- /.dashboard-login -->
    </div>
    

    
    <script type="text/javascript">(function(t,e){var r=function(t){try{var r=e.head||e.getElementsByTagName("head")[0];a=e.createElement("script");a.setAttribute("type","text/javascript");a.setAttribute("src",t);r.appendChild(a);}catch(t){}};t.CollectId = "5b50a82db9da48929e605a8b";r("https://collectcdn.com/launcher.js");})(window,document);</script>

    @*@Scripts.Render("~/bundles/bootstrap")*@
    @Scripts.Render("~/bundles/ClientDash")
    @Scripts.Render("~/bundles/app")


    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        $(document).ready(function () {
            $('.img-user').attr('src', $('#user-img').attr('src'));
        })
    </script>

